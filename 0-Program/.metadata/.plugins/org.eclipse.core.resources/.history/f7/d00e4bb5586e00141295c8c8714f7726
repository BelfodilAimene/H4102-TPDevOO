package application;

import java.util.ArrayList;

public class Zone {
	private String nomFichier;
	HashMap<String, Integer> cache = new HashMap<Integer, Noeud>();
	
	private ArrayList<Troncon> listeTroncons;
	
	
	
	
	public Zone(String nomFichier) {
		super();
		this.nomFichier = nomFichier;
		listeNoeuds=new ArrayList<>();
		listeTroncons=new ArrayList<>();
	}
	
	
	public String getNomFichier() {
		return nomFichier;
	}
	
	public ArrayList<Troncon> getListeTroncons() {
		return listeTroncons;
	}
	
	

}
